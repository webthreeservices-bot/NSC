    "express-rate-limit": "^7.4.0",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lenis": "^1.3.13",
    "lucide-react": "^0.441.0",
    "next": "^16.0.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.15",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.2",
    "qrcode": "^1.5.4",
    "rate-limit-redis": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "recharts": "^2.12.7",
    "redis": "^4.7.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^1.5.0",
    "speakeasy": "^2.0.0",
    "swiper": "^12.0.3",
